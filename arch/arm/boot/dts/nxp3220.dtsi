/ {
	aliases {
		pinctrl0 = &pinctrl_0;
	};

	pinctrl_0: pinctrl@20180000 {
		compatible = "nexell,sip_s31nx-pinctrl";
		reg = <0x20180000 0x1000
			0x20190000 0x1000
			0x201a0000 0x1000
			0x201b0000 0x1000
			0x201c0000 0x1000
			0x2008d000 0x1000>;
		#address-cells = <1>;
		#size-cells = <1>;
		interrupts = <0 45 0>,
			<0 46 0>,
			<0 47 0>,
			<0 48 0>,
			<0 49 0>,
			<0 101 0>,
	};

};

#include "nxp3220-pinctrl.dtsi"
